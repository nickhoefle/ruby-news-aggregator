<h2>The Hill Items</h2>
<ul>
  <% @thehill_items.each do |item| %>
    <li>
      <h3><%= item[:title] %></h3>
      <img src="<%= item[:image_url] %>" alt="Image">
      <p><%= item[:description] %></p>
      <p>Published: <%= item[:pub_date] %></p>
      <a href="<%= item[:link] %>">Read More</a>
    </li>
  <% end %>
</ul>

